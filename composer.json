{
    "name": "rawphp/raw-mail",
    "type": "library",
    "description": "RawMail is a wrapper around PHPMailer and is used by RawPHP framework and other applications.",
    "keywords": [ "email", "smtp" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "rawphp/raw-support": "dev-master",
        "rawphp/raw-dispatcher": "dev-master",
        "phpmailer/phpmailer": "v5.2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawMail\\": "src/RawPHP/RawMail/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
