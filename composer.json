    "keywords": [ "email", "smtp" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
